{
  "name": "@jinyaoma/vuepress-theme-jext",
  "version": "0.0.1-20201219.1417",
  "description": "Documentation-blog Theme Combo for VuePress 1.x",
  "keywords": [
    "vuepress",
    "vuepress-theme",
    "jext"
  ],
  "author": {
    "name": "jinyaoMa",
    "email": "jinyao.ma@outlook.com",
    "url": "https://github.com/jinyaoMa"
  },
  "license": "MIT",
  "repository": "git@github.com:jinyaoMa/vuepress-theme-jext.git",
  "main": "$theme/index.js",
  "scripts": {
    "vuepress:dev": "vuepress dev $_docs",
    "vuepress:build": "vuepress build $_docs",
    "vuepress:deploy": "deploy.sh",
    "icons:dev": "node $icons/generate.js",
    "icons:build": "vue-cli-service build --target lib --name j-icon --dest lib/icons $icons/index.js",
    "components:build": "vue-cli-service build --target lib --name jext --dest lib/components $components/index.js",
    "usenpm": "npmuser.sh npm",
    "usegithub": "npmuser.sh github",
    "publish": "npm publish --access=public"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@vuepress/plugin-blog": "^1.9.2",
    "axios": "^0.21.0",
    "md5": "^2.3.0",
    "vuepress": "^1.7.1",
    "vuepress-plugin-mathjax": "^1.2.8",
    "vuepress-plugin-social-share": "^1.0.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-service": "~4.5.0",
    "font-carrier": "^0.2.0",
    "markdown-it-footnote": "^3.0.2",
    "mini-css-extract-plugin": "^1.2.1"
  }
}
